                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.475*"></RowDefinition>
                    <RowDefinition Height="0.525*"></RowDefinition>
                </Grid.RowDefinitions>
